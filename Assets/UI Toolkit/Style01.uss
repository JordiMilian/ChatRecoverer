.JessMessageStyle {
    width: 80%;
    height: 100%;
    background-color: rgb(178, 65, 166);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 20px;
    border-bottom-left-radius: 0;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    justify-content: flex-end;
    align-items: flex-start;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    flex-direction: row-reverse;
}

.LauraMessageStyle {
    width: 80%;
    height: 100%;
    background-color: rgb(255, 136, 196);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 20px;
    border-bottom-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 0;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    justify-content: flex-end;
    align-items: flex-start;
    flex-direction: row;
}

.JessContainerStyle {
    width: 750px;
    height: 60px;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(255, 255, 255, 0.13);
    margin-left: 2px;
    margin-right: 2px;
    margin-top: 2px;
    margin-bottom: 2px;
    padding-left: 6px;
    padding-right: 6px;
    padding-top: 6px;
    padding-bottom: 6px;
    flex-shrink: 0;
    align-items: flex-start;
    flex-direction: row;
}

.LauraContainerSyle {
    width: 750px;
    height: 100px;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(255, 255, 255, 0.13);
    margin-left: 2px;
    margin-right: 2px;
    margin-top: 2px;
    margin-bottom: 2px;
    padding-left: 6px;
    padding-right: 6px;
    padding-top: 6px;
    padding-bottom: 6px;
    align-items: flex-end;
    flex-shrink: 0;
}

.TextStyle {
    font-size: 100%;
    color: rgb(0, 0, 0);
    white-space: normal;
    flex-wrap: wrap;
    width: auto;
}

.TimeSTyle {
    color: rgba(0, 0, 0, 0.43);
    font-size: 75%;
    -unity-font-style: italic;
}

.TimeContainer {
    height: 0;
    align-items: flex-end;
    justify-content: space-between;
    flex-direction: column;
    flex-shrink: 0;
    width: auto;
}
